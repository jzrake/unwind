Cell[CellGroupData[{
Cell["\<\
Write a tabluated F(b, \[Chi]), (the function that contains all the polylogs) \
to a file over some range of interesting parameters\
\>", "Subsubsection",
 CellChangeTimes->{{3.577712285060193*^9, 3.577712369140255*^9}}],

Cell[BoxData[{
 RowBox[{
  RowBox[{"p", "=", "4"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"ms", "=", "1"}], ";"}]}], "Input",
 CellChangeTimes->{{3.577712751497921*^9, 3.577712754548678*^9}, {
  3.577718002102894*^9, 3.57771800501531*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"For", "[", 
  RowBox[{
   RowBox[{"n", "=", "0"}], ",", 
   RowBox[{"n", "<", "10"}], ",", " ", 
   RowBox[{"n", "++"}], ",", "\[IndentingNewLine]", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"b", "=", 
     RowBox[{
      FractionBox["n", "5"], 
      FractionBox["1", 
       RowBox[{
        SqrtBox[
         RowBox[{"2", " ", "\[Pi]"}]], " ", "ms"}]]}]}], ";", 
    "\[IndentingNewLine]", "\[IndentingNewLine]", 
    RowBox[{"\[Chi]in", "=", 
     FractionBox["1", "10"]}], ";", "\[IndentingNewLine]", 
    RowBox[{"\[Chi]fin", "=", "10"}], ";", "\[IndentingNewLine]", 
    RowBox[{"nsteps", "=", "600"}], ";", "\[IndentingNewLine]", 
    RowBox[{"list", "=", 
     RowBox[{"Table", "[", 
      RowBox[{
       RowBox[{"\[Chi]in", " ", 
        SuperscriptBox[
         RowBox[{"(", 
          FractionBox["\[Chi]fin", "\[Chi]in"], ")"}], 
         RowBox[{"a", "/", "nsteps"}]]}], ",", 
       RowBox[{"{", 
        RowBox[{"a", ",", "0", ",", "nsteps"}], "}"}]}], "]"}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"polyt", "=", 
     RowBox[{"Table", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
         RowBox[{"list", "[", 
          RowBox[{"[", "i", "]"}], "]"}], ",", 
         RowBox[{
          FractionBox["1", 
           RowBox[{"Gamma", "[", 
            RowBox[{
             FractionBox["p", "2"], "+", "1"}], "]"}]], " ", 
          RowBox[{"NIntegrate", "[", 
           RowBox[{
            FractionBox[
             SuperscriptBox["t", 
              FractionBox["p", "2"]], 
             RowBox[{"1", "+", 
              RowBox[{"Exp", "[", 
               RowBox[{"t", "-", 
                FractionBox[
                 RowBox[{
                  SuperscriptBox["\[Pi]", "2"], "-", 
                  SuperscriptBox["b", "2"]}], 
                 RowBox[{"list", "[", 
                  RowBox[{"[", "i", "]"}], "]"}]]}], "]"}]}]], ",", 
            RowBox[{"{", 
             RowBox[{"t", ",", "0", ",", "\[Infinity]"}], "}"}], ",", 
            RowBox[{"{", 
             RowBox[{
              RowBox[{"PrecisionGoal", "\[Rule]", "3"}], ",", 
              RowBox[{"MaxRecursion", "\[Rule]", "50"}]}], "}"}]}], "]"}]}]}],
         "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"i", ",", "1", ",", 
         RowBox[{"nsteps", "+", "1"}]}], "}"}]}], "]"}]}], ";", 
    "\[IndentingNewLine]", "\[IndentingNewLine]", 
    RowBox[{"\[Chi]min", "=", 
     SuperscriptBox["10", 
      RowBox[{"-", "4"}]]}], ";", "\[IndentingNewLine]", 
    RowBox[{"\[Chi]max", "=", 
     FractionBox["1", "10"]}], ";", "\[IndentingNewLine]", 
    RowBox[{"list2", "=", 
     RowBox[{"Table", "[", 
      RowBox[{
       RowBox[{"\[Chi]min", " ", 
        SuperscriptBox[
         RowBox[{"(", 
          FractionBox["\[Chi]max", "\[Chi]min"], ")"}], 
         RowBox[{"b", "/", "nsteps"}]]}], ",", 
       RowBox[{"{", 
        RowBox[{"b", ",", "0", ",", "nsteps"}], "}"}]}], "]"}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"appr", "=", 
     RowBox[{"Table", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
         RowBox[{"list2", "[", 
          RowBox[{"[", "j", "]"}], "]"}], ",", 
         RowBox[{
          SuperscriptBox[
           RowBox[{"(", 
            FractionBox[
             RowBox[{
              SuperscriptBox["\[Pi]", "2"], "-", 
              SuperscriptBox["b", "2"]}], 
             RowBox[{"list2", "[", 
              RowBox[{"[", "j", "]"}], "]"}]], ")"}], 
           RowBox[{
            FractionBox["p", "2"], "+", "1"}]], "/", 
          RowBox[{"Gamma", "[", 
           RowBox[{
            FractionBox["p", "2"], "+", "2"}], "]"}]}]}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"j", ",", "1", ",", 
         RowBox[{"nsteps", "+", "1"}]}], "}"}]}], "]"}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"\[Chi]full", "=", 
     RowBox[{"Join", "[", 
      RowBox[{
       RowBox[{"list2", "[", 
        RowBox[{"[", 
         RowBox[{"1", ";;", "nsteps"}], "]"}], "]"}], ",", "list"}], "]"}]}], 
    ";", "\[IndentingNewLine]", 
    RowBox[{"full", "=", 
     RowBox[{"Join", "[", 
      RowBox[{
       RowBox[{"appr", "[", 
        RowBox[{"[", 
         RowBox[{"1", ";;", "nsteps"}], "]"}], "]"}], ",", "polyt"}], "]"}]}],
     ";", "\[IndentingNewLine]", 
    RowBox[{"intfull", "=", 
     RowBox[{"Interpolation", "[", "full", "]"}]}], ";", 
    "\[IndentingNewLine]", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"ftemp", "[", "\[Chi]_", "]"}], "=", 
     RowBox[{"(", 
      RowBox[{
       RowBox[{"intfull", "[", "\[Chi]", "]"}], "+", 
       RowBox[{"8", " ", 
        RowBox[{"PolyLog", "[", 
         RowBox[{
          RowBox[{
           FractionBox["p", "2"], "+", "1"}], ",", 
          SuperscriptBox["\[ExponentialE]", 
           RowBox[{"-", 
            FractionBox[
             SuperscriptBox["b", "2"], "\[Chi]"]}]]}], "]"}]}], "-", 
       RowBox[{"36", " ", 
        RowBox[{"PolyLog", "[", 
         RowBox[{
          RowBox[{
           FractionBox["p", "2"], "+", "1"}], ",", 
          RowBox[{"-", 
           SuperscriptBox["\[ExponentialE]", 
            RowBox[{"-", 
             FractionBox[
              RowBox[{
               SuperscriptBox["b", "2"], "+", 
               SuperscriptBox["\[Pi]", "2"]}], "\[Chi]"]}]]}]}], "]"}]}], "+", 
       RowBox[{"128", " ", 
        RowBox[{"PolyLog", "[", 
         RowBox[{
          RowBox[{
           FractionBox["p", "2"], "+", "1"}], ",", 
          SuperscriptBox["\[ExponentialE]", 
           RowBox[{"-", 
            FractionBox[
             RowBox[{
              SuperscriptBox["b", "2"], "+", 
              RowBox[{"2", " ", 
               SuperscriptBox["\[Pi]", "2"]}]}], "\[Chi]"]}]]}], "]"}]}]}], 
      ")"}]}], ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
    RowBox[{"ftempTableLL", " ", "=", " ", 
     RowBox[{"Table", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
         RowBox[{"N", "[", 
          RowBox[{
           RowBox[{"Log10", "[", 
            RowBox[{"\[Chi]full", "[", 
             RowBox[{"[", "m", "]"}], "]"}], "]"}], ",", "12"}], "]"}], ",", 
         RowBox[{"N", "[", 
          RowBox[{
           RowBox[{"Log10", "[", 
            RowBox[{"ftemp", "[", 
             RowBox[{"\[Chi]full", "[", 
              RowBox[{"[", "m", "]"}], "]"}], "]"}], "]"}], ",", "12"}], 
          "]"}]}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"m", ",", "1", ",", 
         RowBox[{
          RowBox[{"2", "*", "nsteps"}], "+", "1"}]}], "}"}]}], "]"}]}], ";", 
    "\[IndentingNewLine]", "\[IndentingNewLine]", 
    RowBox[{"parameters", " ", "=", " ", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"N", "[", 
        RowBox[{"b", ",", "12"}], "]"}], ",", " ", "p"}], "}"}]}], ";", 
    "\[IndentingNewLine]", "\[IndentingNewLine]", 
    RowBox[{"FullTableLL", " ", "=", " ", 
     RowBox[{"Join", "[", 
      RowBox[{"parameters", ",", " ", "ftempTableLL"}], "]"}]}], ";", 
    "\[IndentingNewLine]", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"SetDirectory", "[", 
      RowBox[{"NotebookDirectory", "[", "]"}], "]"}], "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"Export", "[", 
      RowBox[{
       RowBox[{"StringJoin", "[", 
        RowBox[{"\"\<ftempLL-b\>\"", ",", " ", 
         RowBox[{"ToString", "[", "n", "]"}], ",", " ", "\"\<over5-p\>\"", 
         ",", 
         RowBox[{"ToString", "[", "p", "]"}], ",", "\"\<.dat\>\""}], "]"}], 
       ",", " ", "FullTableLL"}], "]"}]}]}]}], "\[IndentingNewLine]", 
  "]"}]], "Input",
